{
  "name": "@nio/ui-kit",
  "version": "1.0.1",
  "description": "A toolkit for building nio front ends. Based on ReactJS, Reactstrap, VictoryCharts, and CSS Modules",
  "deploymentUrl": "uikit.niolabs.com",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src && eslint docs && sass-lint --verbose",
    "build": "rimraf dist && babel --quiet ./src --out-dir ./dist && cpy \"**/*.scss\" ../dist --parents --cwd=src",
    "publish-package": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nioinnovation/ui-kit.git"
  },
  "author": "nioinnovation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nioinnovation/ui-kit/issues"
  },
  "homepage": "https://github.com/nioinnovation/ui-kit#readme",
  "dependencies": {
    "@nio/icons": "^1.2.0",
    "bootstrap": "=4.0.0-alpha.6",
    "d3": "^4.11.0",
    "highlight.js": "^9.12.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-css-themr": "^2.1.2",
    "react-dom": "^15.6.2",
    "react-rangeslider": "^2.2.0",
    "react-toggle-button": "^2.1.0",
    "reactstrap": "^5.0.0-alpha.3",
    "victory": "^0.24.0"
  },
  "bundledDependencies": [
    "@nio/icons",
    "bootstrap",
    "d3",
    "highlight.js",
    "lodash",
    "moment",
    "prop-types",
    "react",
    "react-css-themr",
    "react-dom",
    "react-toggle-button",
    "reactstrap",
    "victory"
  ],
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "cpy-cli": "^1.0.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-postcss": "^2.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1"
  }
}
