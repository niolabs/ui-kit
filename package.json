{
  "name": "@nio/ui-kit",
  "version": "1.6.9",
  "description": "A toolkit for building nio front ends. Based on ReactJS, Reactstrap, VictoryCharts, and CSS Modules",
  "deploymentUrl": "uikit.niolabs.com",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node-sass --watch --recursive --importer node_modules/node-sass-magic-importer/dist/cli.js -o dist src",
    "lint": "npm run lint-src && npm run lint-ui && npm run lint-sass",
    "lint-src": "eslint src --fix",
    "lint-ui": "eslint ui --fix",
    "lint-sass": "sass-lint --verbose",
    "build": "rimraf dist && babel ./src --out-dir ./dist && npm run copy-icons && npm run process-scss",
    "process-scss": "node-sass --recursive --importer node_modules/node-sass-magic-importer/dist/cli.js -o dist src",
    "copy-icons": "cpy 'nioicons.*' ../../../../dist/components/Icon --parents --cwd=node_modules/@nio/icons/dist/",
    "publish-package": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nioinnovation/ui-kit.git"
  },
  "author": "nioinnovation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nioinnovation/ui-kit/issues"
  },
  "homepage": "https://github.com/nioinnovation/ui-kit#readme",
  "dependencies": {
    "@nio/icons": "^1.9.0",
    "bootstrap": "4.3.1",
    "frappe-charts": "^1.1.0",
    "highlight.js": "^9.14.2",
    "node-sass": "^4.11.0",
    "node-sass-import": "^2.0.1",
    "node-sass-magic-importer": "^5.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-charts": "^2.0.0-alpha.12",
    "react-clock": "^2.3.0",
    "react-css-themr": "^2.1.2",
    "react-dom": "^16.8.3",
    "react-motion": "^0.5.2",
    "react-rangeslider": "^2.2.0",
    "react-toggle-button": "^2.2.0",
    "reactstrap": "^7.1.0"
  },
  "bundledDependencies": [
    "@nio/icons",
    "bootstrap",
    "frappe-charts",
    "highlight.js",
    "prop-types",
    "react",
    "react-css-themr",
    "react-dom",
    "react-motion",
    "react-rangeslider",
    "react-toggle-button",
    "reactstrap"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "@babel/traverse": "7.2.3",
    "autoprefixer": "^9.4.8",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-imports": "^1.5.1",
    "cpy-cli": "^2.0.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-postcss": "^3.0.7",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "rimraf": "^2.6.3",
    "sass-lint": "^1.12.1"
  }
}
