{
  "name": "@nio/ui-kit",
  "version": "2.1.2",
  "description": "A toolkit for building nio front ends. Based on ReactJS, Reactstrap, ApexCharts, and CSS Modules",
  "deploymentUrl": "uikit.niolabs.com",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node-sass --watch --recursive --importer node_modules/node-sass-magic-importer/dist/cli.js -o dist src",
    "lint": "npm run lint-src && npm run lint-ui && npm run lint-sass",
    "lint-src": "eslint src --fix",
    "lint-ui": "eslint ui --fix",
    "lint-sass": "sass-lint --verbose",
    "build": "rimraf dist && babel ./src --out-dir ./dist && npm run copy-icons && npm run process-scss",
    "process-scss": "node-sass --recursive --importer node_modules/node-sass-magic-importer/dist/cli.js -o dist src",
    "copy-icons": "cpy 'nioicons.*' ../../../../dist/components/Icon --parents --cwd=node_modules/@nio/icons/dist/",
    "publish-package": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nioinnovation/ui-kit.git"
  },
  "author": "nioinnovation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nioinnovation/ui-kit/issues"
  },
  "homepage": "https://github.com/nioinnovation/ui-kit#readme",
  "dependencies": {
    "@nio/icons": "^1.10.0",
    "apexcharts": "^3.11.1",
    "bootstrap": "4.4.1",
    "highlight.js": "^9.17.1",
    "node-sass": "^4.13.0",
    "node-sass-import": "^2.0.1",
    "node-sass-magic-importer": "^5.3.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-apexcharts": "^1.3.3",
    "react-clock": "^2.4.0",
    "react-css-themr": "^2.1.2",
    "react-dom": "^16.12.0",
    "react-motion": "^0.5.2",
    "react-rangeslider": "^2.2.0",
    "react-toggle-button": "^2.2.0",
    "reactstrap": "^8.2.0"
  },
  "bundledDependencies": [
    "@nio/icons",
    "bootstrap",
    "apexcharts",
    "highlight.js",
    "prop-types",
    "react",
    "react-apexcharts",
    "react-css-themr",
    "react-clock",
    "react-dom",
    "react-motion",
    "react-rangeslider",
    "react-toggle-button",
    "reactstrap"
  ],
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-function-bind": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.7.7",
    "@babel/traverse": "7.7.4",
    "@babel/types": "^7.7.4",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-imports": "^2.0.0",
    "cpy-cli": "^3.0.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-postcss": "^3.0.7",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "rimraf": "^3.0.0",
    "sass-lint": "^1.13.1"
  }
}
